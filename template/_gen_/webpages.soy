{template .MainLauncher}
<div id="MainLauncherDivId" class="LogicalHide">
	<div id ="MainLauncherCenterDiv" >
		
		<div class="row">
			<h1 id="MainLauncherTitle" class="pageTitle" >I want to&hellip;</h1>
		</div>
		<div class="row"> 
			<div class="twocol largeIconSpan" id="launcherShowGetHelpSpanId"><a id="launcherShowHelp" onclick="app.showPage('AppLogger');">
